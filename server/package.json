{
  "name": "web-asambleas",
  "version": "1.0.0",
  "description": "programa para conexion en linea de propietarios en las asambleas",
  "main": "src/index.js",
  "scripts": {
    "frontend:install": "cd cliente && npm install",
    "frontend:build": "cd cliente && npm run build",
    "backend:install": "cd src && npm install",
    "dev": "concurrently \"cd cliente && npm run dev\" \"cd src && nodemon index.js\"",
    "build": "npm run frontend:build",
    "start": "node src/index.js"
  },
  "author": "Ivan Rivas",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "body-parser": "^1.20.3",
    "chart.js": "^4.4.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "resend": "^4.2.0",
    "socket.io": "^4.6.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "bufferutil": "^4.0.7",
    "nodemon": "^2.0.22",
    "utf-8-validate": "^5.0.10"
  }
}
